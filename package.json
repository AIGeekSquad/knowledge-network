{
  "name": "knowledge-network-workspace",
  "version": "0.1.0",
  "private": true,
  "description": "A TypeScript library extending d3.js for knowledge graph visualization",
  "type": "module",
  "scripts": {
    "build": "pnpm --recursive --filter='!./packages/examples' run build",
    "dev": "pnpm --recursive --parallel run dev",
    "test": "pnpm --recursive run test",
    "lint": "pnpm --recursive run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm --recursive run clean && rm -rf node_modules"
  },
  "keywords": [
    "d3",
    "knowledge-graph",
    "visualization",
    "typescript"
  ],
  "author": "AIGeekSquad",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.10.1",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
