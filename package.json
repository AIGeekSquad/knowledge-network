{
  "name": "knowledge-network-workspace",
  "version": "0.1.0",
  "private": true,
  "description": "A TypeScript library extending d3.js for knowledge graph visualization",
  "type": "module",
  "scripts": {
    "build": "pnpm --recursive --filter='!./packages/examples' run build",
    "dev": "pnpm --recursive --parallel run dev",
    "test": "pnpm --recursive run test",
    "lint": "pnpm --recursive run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm --recursive run clean && rm -rf node_modules"
  },
  "keywords": [
    "d3",
    "knowledge-graph",
    "visualization",
    "typescript"
  ],
  "author": "AIGeekSquad",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.18.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/ui": "^2.1.9",
    "d3": "^7.9.0",
    "eslint": "^9.36.0",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.23.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^2.1.9"
  }
}