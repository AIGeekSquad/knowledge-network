{
  "name": "@aigeeksquad/knowledge-network-demo-suite",
  "version": "2.0.0",
  "private": true,
  "description": "Next-generation interactive showcase platform for knowledge-network library",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "playwright test --grep accessibility",
    "test:performance": "playwright test --grep performance",
    "test:module": "vitest --run src/modules",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "clean": "rm -rf dist node_modules .vite"
  },
  "keywords": [
    "knowledge-graph",
    "visualization",
    "demo",
    "showcase",
    "d3",
    "typescript",
    "gpu-acceleration",
    "semantic-ai",
    "accessibility",
    "mobile",
    "performance",
    "competitive-analysis"
  ],
  "author": "AIGeekSquad",
  "license": "MIT",
  "dependencies": {
    "@aigeeksquad/knowledge-network": "workspace:*",
    "d3": "^7.9.0",
    "monaco-editor": "^0.45.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.18.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "axe-core": "^4.10.2",
    "eslint": "^9.36.0",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.3.6",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}