{
  "name": "@aigeeksquad/knowledge-network",
  "version": "0.1.0",
  "description": "Modern TypeScript library for interactive knowledge graph visualizations with advanced edge bundling, similarity clustering, and ontology-aware relationships",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "d3",
    "knowledge-graph",
    "visualization",
    "typescript",
    "network",
    "graph",
    "edge-bundling",
    "force-directed",
    "interactive",
    "clustering",
    "ontology",
    "semantic-network"
  ],
  "author": "AIGeekSquad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AIGeekSquad/KnowledgeGraphRenderer.git"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@swc/core": "^1.10.1",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/ui": "^2.1.9",
    "d3": "^7.9.0",
    "d3-selection": "^3.0.0",
    "eslint": "^9.18.0",
    "jsdom": "^27.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.45.0",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "rxjs": "^7.8.2"
  }
}
